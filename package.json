{
  "name": "@hoody-network/html-rewriter",
  "version": "0.3.0",
  "description": "Node.js-based derivative of the worker-tools/html-rewriter repo",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@types/node": "^18.11.8",
    "symlink-dir": "^5.0.1",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "buildBase": "make dist",
    "buildWrapper": "tsc",
    "build": "npm run buildBase && npm run buildWrapper",
    "postinstall": "npx symlink-dir vendor dist/vendor"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Hoody-Network/html-rewriter"
  },
  "author": "Hoody-Network",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hoody-Network/html-rewriter/issues"
  },
  "homepage": "https://github.com/Hoody-Network/html-rewriter#readme"
}
